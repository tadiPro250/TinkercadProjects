#include<iostream>
using namespace std;
int main()
{
    int num;
    cout<<"Enter a number:";
    cin>>num;
    cout<<"prime factors of "<<num<<" are: ";
    for(int i=2;i<=num;i++)
    {
        while(num%i==0)
        {
            cout<<i<<" ";
            num=num/i;
        }
    }
}


