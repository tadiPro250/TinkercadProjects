#include<iostream>
using namespace std;
class student{
    int id;
    string name;
public:
    student(){
        cout<<"Enter Name of student: "<<endl;
        cin>>name;
        cout<<"Enter ID of student: "<<endl;
        cin>>id;

    }
    void detail(){
        cout<<"Name of student is: "<<name<<endl;
        cout<<"ID of student is: "<<id<<endl;
    }
};
int main(){
student s1;
student s2;
student s3;
s1.detail();
s2.detail();
s3.detail();
return 0;
};